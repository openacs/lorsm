<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Frameset//EN"
   "http://www.w3.org/TR/html4/frameset.dtd">
<html>
<head>
      <title>@course_name@</title>
</head>
<frameset rows="50,*,50" border="2">
<frame src="header?course_name=@course_name@" name="header" title="header" scrolling="no">
<frameset cols="20%, *" frameborder="1" framespacing="3">
<frame frameborder="2" marginwidth="0" marginheight="0" src="toc?man_id=@man_id@" name="frame" title="TOC">
<frame frameborder="2" src="body?course_name=@course_name@" name="body">
</frameset>

<frame src="footer" name="footer" title="footer" scrolling="no">
<noframes>
      <p><br />
  </p>
  </noframes>
</frameset>
</html>